    "@vue/tsconfig": "^0.5.1",
    "css-loader": "^7.1.2",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.26.0",
    "npm-run-all2": "^6.2.0",
    "prettier": "^3.3.2",
    "prettier-plugin-organize-imports": "^4.2.0",
    "style-loader": "^4.0.0",
